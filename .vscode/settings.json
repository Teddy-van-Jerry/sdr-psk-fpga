{
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "-shell-escape",
                "-cd",
                "%DOC%"
            ],
            "env": {
                "TZ": "America/New_York"
            }
        },
        {
            "name": "latexmk (lua)",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdflua",
                "-shell-escape",
                "-cd",
                "%DOC%"
            ],
            "env": {
                "TZ": "America/New_York"
            }
        }
    ],
    "latex-workshop.latex.recipes": [
        {
            "name": "latexmk",
            "tools": [
                "latexmk"
            ],
        },
        {
            "name": "latexmk (lua)",
            "tools": [
                "latexmk (lua)"
            ]
        }
    ],
    "files.eol": "\n",
    "[verilog]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2
    },
    "cSpell.words": [
        "BPSK",
        "QPSK"
    ]
}